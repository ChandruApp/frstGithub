        "print(\"Enter a Number \\n\")\n",
        "num = int(input())\n",
        "rev = 0\n",
        "while num > 0:\n",
        "    rev = (rev*10) + (num %10);\n",
        "    num = num//10\n",
        "print(\"Reverse is: %d \" %rev)"
